{
    "manifest_version": 3,
    "name": "Anki Enforcer",
    "version": "0.1",
    "permissions": [
        "tabs",
        "storage",
        "alarms",
        "webNavigation",
        "activeTab"
    ],
    "host_permissions": [
        "http://127.0.0.1/*"
    ],
    "background": {
        "service_worker": "service_worker.js"
    },
    "chrome_url_overrides": {},
    "web_accessible_resources": [{
        "resources": ["study.html", "study.js"],
        "matches": ["*://*/*"]
    }]
}